{
  "expo": {
    "name": "Atendify2025",
    "slug": "Atendify2025",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#EEEEEE"
    },
    "scheme": "atendify2025",
    "platforms": ["ios", "android"],
    "ios": {
      "bundleIdentifier": "com.atendify2025.app",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#EEEEEE"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.atendify2025.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "28879798-c0c7-4fe9-8bbb-a8fd602b874a"
      }
    }
  }
}
